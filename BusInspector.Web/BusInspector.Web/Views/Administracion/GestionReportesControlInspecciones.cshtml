@model IEnumerable<BusInspector.Dal.Inspector>

@{
    ViewBag.Title = "Control de inspecciones";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Control de Inspecciones</h2>


@if (TempData["Error"] != null)
{
    <div class="alert alert-danger">@TempData["Error"]</div>
}

<div class="col s12">
    <div class="card">
        <div class="card-content">
                <h5 class="text-center">Seleccione el Inspector, Fecha de Inicio y Fin para generar el reporte</h5>
                <br>
                @using (Html.BeginForm("ReportesControlInspecciones", "Administracion", FormMethod.Post))
                {
                    @Html.Partial("_PartialInformesFiltros");
                }
        </div>
    </div>
</div>





